{
  "expo": {
    "name": "Aditya Contacts",
    "slug": "aditya-contacts",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/20260104_195159.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/20260104_195159.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "permissions": [
      "CAMERA",
      "MEDIA_LIBRARY",
      "READ_CONTACTS",
      "WRITE_CONTACTS"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aditya.contacts",
      "infoPlist": {
        "NSContactsUsageDescription": "This app requires access to your contacts to display and manage them."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/20260104_195159.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aditya.contacts",
      "versionCode": 1,
      "permissions": [
        "android.permission.CALL_PHONE",
        "android.permission.READ_PHONE_STATE",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/20260104_195159.png"
    },
    "plugins": [
      "expo-asset",
      "expo-video"
    ],
    "owner": "adithyasai533",
    "extra": {
      "eas": {
        "projectId": "95bcefcc-e013-4a60-87b1-b53bee9b7453"
      }
    }
  }
}